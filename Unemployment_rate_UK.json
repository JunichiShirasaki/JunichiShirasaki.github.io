{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Office for national statistics, UK Unemployment rate (aged 16 and over), yearly change",
  "data": {
    "url": "https://raw.githubusercontent.com/JunichiShirasaki/JunichiShirasaki.github.io/refs/heads/main/Unemployment_rate_UK.csv"
  },
  "title": {"text": "Unemployment rate (aged 16 and over)"},
  "mark": {
    "type": "line",
    "color": "black",
    "point": {"filled": true, "color": "black"}
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "nominal",
      "axis": {
        "title": "year",
        "labelExpr": "substring(datum.value,0,4)",
        "grid": true,
        "labelFontSize": 10,
        "titleFontSize": 13
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "%",
        "titleAngle": 0,
        "titleX": -20,
        "titleY": -10,
        "titleFontSize": 12
      }
    }
  },
  "config": {}
}