{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Office for national statistics, UK homicide, yearly change",
  "data": {
    "url": "https://raw.githubusercontent.com/JunichiShirasaki/JunichiShirasaki.github.io/refs/heads/main/Figure_2__Homicide_decreased_by_6%25_in_the_last_year_.csv"
  },
  "title": {
    "text": "England and Wales, year ending (YE) March 2003 to YE March 2025"
  },
  "mark": {
    "type": "line",
    "color": "blue",
    "point": {"filled": true, "color": "blue"}
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "nominal",
      "axis": {
        "title": "year",
        "labelExpr": "substring(datum.value,0,4)",
        "grid": true,
        "labelFontSize": 10,
        "titleFontSize": 13
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "number of homicide"}
    }
  },
  "config": {}
}